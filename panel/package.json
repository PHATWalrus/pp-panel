{
  "name": "pp-banging-panel",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.2.0",
  "scripts": {
    "sync:schema": "cp ../../prisma/schema.prisma ./prisma/schema.prisma",
    "prisma:generate": "bunx prisma generate --schema=./prisma/schema.prisma",
    "dev": "bun run prisma:generate && bunx next dev -p 3001",
    "build": " bun run prisma:generate && bunx next build",
    "start": "bunx next start -p 3001",
    "lint": "bunx next lint"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "6",
    "@tanstack/react-query": "^5.90.21",
    "@trpc/client": "^11.10.0",
    "@trpc/react-query": "^11.10.0",
    "@trpc/server": "^11.10.0",
    "next": "^16.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "sass": "^1.84.0",
    "superjson": "^2.2.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "prisma": "6"
  }
}
